<link rel="stylesheet" href="{{ asset('css/login.css') }}" />
{% block body %}
    {{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.plainPassword.first) }}
    {{ form_row(form.plainPassword.second) }}
    <input type="submit" name="submit" value="Sign in">
    {{ form_end(form) }}
{% endblock %}